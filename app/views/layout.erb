  <head>
    <meta charset="utf-8" />
    <meta http-equiv="X-UA-Compatible" content="IE=edge, chrome=1" />

    <title>INSTYLECART</title>

    <meta name="viewport" content="width=device-width, initial-scale=1.0" />
     <!-- Compiled and minified CSS -->
    <link rel="stylesheet" href="https://cdnjs.cloudflare.com/ajax/libs/materialize/1.0.0/css/materialize.min.css">

  </head>
  <body style="background-image: url('https://pe-images.s3.amazonaws.com/basics/cs6/layers/background-layer-action/adobe-stock-image.jpg')">
      
      <!-- navigation -->
      <div class="container">
       <nav>
        <div>
         <ul class="right">
          <% unless logged_in? %>
            <li><a href="/signup">CREATE ACCOUNT</a></li>
            <li><a href="/login">LOGIN</a></li>
          <% else %>
            <li><%= current_user.username %></li>
            <li><a href="/logout">LOGOUT</a></li>
          <% end %>
        </ul>
          <div>
          <ul>
          <li><a href="http://127.0.0.1:9393/items">HOME</a></li>
          <li><a href="http://127.0.0.1:9393/items/new">ADD NEW ITEMS</a></li>
          <ul>
          </div>
       </div>
      </nav>
    <!-- end navigation -->
    
    <!-- error messages -->
        <% if flash[:error] %>
          <% flash[:error].each do |error| %>
            <div>
              <%= error %>
            </div>
          <% end %>
        <% end %>
      <!--  end error messages -->

        <%= yield %>
      
      <!-- footer -->
        <footer class="card-panel">
        <center>
        <div class="black-text">
            <small>&copy; 2020 <strong>GRACE KIM goooyoon@gmail.com</strong></small>
        </div>
        </center>
        </footer>
     <!-- end footer -->   

    <script src="http://ajax.googleapis.com/ajax/libs/jquery/1/jquery.min.js"></script>
