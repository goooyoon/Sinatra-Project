    <header> 
        <div style="font-size:60px; color:coral;"><%= current_user.username %>'s Items</div> 
    </header> 
    <br>
    <br>
   <div>
<% @items.each do |item| %>
          <p class="blue-text text-darken-2">Name: <%= item.name %></p>
          <p class="blue-text text-darken-2">Brand: <%= item.brand %></p>
          <p class="blue-text text-darken-2">Price: $<%= item.price %></p>
          <a href="<%= item.link %>">Link to website</a>
          <br>
  <a href="/items/<%=item.id%>/edit">Edit</a>
  <br>
  <br>
  <% end %>
  </div>